<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_margin="5dp"
    android:background="@drawable/rectangle_box"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/Member_IDtv"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:text="SR. No. 1"
            android:padding="10dp"
            android:textStyle="bold"
            android:background="@drawable/submit_button"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:textSize="14dp" />
        <ImageView
            android:id="@+id/pdf_img"
            android:layout_width="35dp"
            android:src="@drawable/pdf"
            android:padding="5dp"
            android:layout_marginRight="5dp"
            android:layout_centerVertical="true"
            android:background="@drawable/logo"
            android:layout_alignParentRight="true"
            android:layout_height="35dp">
        </ImageView>
    </RelativeLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:elevation="5dp"
        android:gravity="center"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:padding="8dp">
                <TextView
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"

                    android:text="Member Name"
                    android:textColor="@color/white"
                    android:textSize="12dp" />
                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey" />
                <TextView
                    android:id="@+id/MemberNametv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"

                    android:text="N/A"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/white"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:padding="8dp">

                <TextView
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"

                    android:text="Deposit Amount"
                    android:textColor="@color/white"
                    android:textSize="12dp" />

                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey" />

                <TextView
                    android:id="@+id/Package_nametv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"

                    android:text="N/A"

                    android:layout_marginLeft="10dp"
                    android:textColor="@color/white"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"

                android:padding="8dp">

                <TextView
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"

                    android:text="Maturity"
                    android:textColor="@color/white"
                    android:textSize="12dp" />

                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"

                    android:text="N/A"
                    android:id="@+id/TotalDaytv"

                    android:layout_marginLeft="10dp"
                    android:textColor="@color/red"
                    android:textSize="12dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:padding="8dp">

                <TextView
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"

                    android:text="Enter Date"
                    android:textColor="@color/white"
                    android:textSize="12dp" />
                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey" />


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"

                    android:text="N/A"
                    android:textColor="@color/white"
                    android:id="@+id/TotalReturnAmttv"

                    android:layout_marginLeft="10dp"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:padding="8dp">

                <TextView
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"

                    android:text="Enter Time"
                    android:textColor="@color/white"
                    android:textSize="12dp" />
                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey" />


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"

                    android:text="N/A"
                    android:textColor="@color/white"
                    android:id="@+id/rewardPointtv"

                    android:layout_marginLeft="10dp"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"

                android:padding="8dp">

                <TextView
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"

                    android:text="Pay Mode"
                    android:textColor="@color/white"
                    android:textSize="12dp" />
                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey" />


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"

                    android:text="N/A"
                    android:textColor="@color/white"
                    android:id="@+id/ActivationDatetv"

                    android:layout_marginLeft="10dp"
                    android:textSize="12dp" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:padding="8dp">

                <TextView
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"

                    android:text="Plan Name"
                    android:textColor="@color/white"
                    android:textSize="12dp" />
                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey" />


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"

                    android:text="N/A"
                    android:textColor="@color/white"
                    android:id="@+id/ValidityDatetv"

                    android:layout_marginLeft="10dp"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/planll"

                android:padding="8dp">

                <TextView
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"

                    android:text="Plan Duration"
                    android:textColor="@color/white"
                    android:textSize="12dp" />
                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey" />


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"

                    android:text="N/A"
                    android:textColor="@color/white"
                    android:id="@+id/ValidityDatetv1"

                    android:layout_marginLeft="10dp"
                    android:textSize="12dp" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:id="@+id/only_dd_ll"
                android:orientation="vertical"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:padding="8dp">
                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"

                        android:text="Next Due Date"
                        android:textColor="@color/white"
                        android:textSize="12dp" />
                    <View
                        android:layout_width="0.2dp"
                        android:layout_height="match_parent"
                        android:background="@color/grey" />


                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical"

                        android:text="N/A"
                        android:textColor="@color/white"
                        android:id="@+id/NextDueDatetv"

                        android:layout_marginLeft="10dp"
                        android:textSize="12dp" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:padding="8dp">

                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"

                        android:text="Pay Date"
                        android:textColor="@color/white"
                        android:textSize="12dp" />
                    <View
                        android:layout_width="0.2dp"
                        android:layout_height="match_parent"
                        android:background="@color/grey" />


                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical"

                        android:text="N/A"
                        android:textColor="@color/white"
                        android:id="@+id/PayDatetv"

                        android:layout_marginLeft="10dp"
                        android:textSize="12dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:padding="8dp">

                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"

                        android:text="Balance Amount"
                        android:textColor="@color/white"
                        android:textSize="12dp" />
                    <View
                        android:layout_width="0.2dp"
                        android:layout_height="match_parent"
                        android:background="@color/grey" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical"

                        android:text="N/A"
                        android:textColor="@color/white"
                        android:id="@+id/BalanceAmttv"

                        android:layout_marginLeft="10dp"
                        android:textSize="12dp" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/emi_tv"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:gravity="center"
                android:textAlignment="center"
                android:background="@drawable/otp_btn"
                android:layout_margin="5dp"
                android:layout_gravity="end"
                android:text="View EMI Chart"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:textSize="14dp" />

            <View
                android:layout_width="match_parent"
                android:background="@color/grey"
                android:layout_height="0.1dp">


            </View>

        </LinearLayout>
    </LinearLayout>

</LinearLayout>